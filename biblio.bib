@article{Biancardi2024,
  author    = {Beatrice Biancardi and Mathieu Chollet and Chloé Clavel},
  title     = {Introducing the 3MT\_French dataset to investigate the timing of public speaking judgements},
  journal   = {Language Resources and Evaluation},
  year      = {2024},
  volume    = {},
  number    = {},
  pages     = {},
  doi       = {10.1007/s10579-023-09709-5},
  url       = {https://doi.org/10.1007/s10579-023-09709-5},
  note      = {Published: 2024/03/23},
  abstract  = {In most public speaking datasets, judgements are given after watching the entire performance, or on thin slices randomly selected from the presentations, without focusing on the temporal location of these slices. This does not allow to investigate how people’s judgements develop over time during presentations. This contrasts with primacy and recency theories, which suggest that some moments of the speech could be more salient than others and contribute disproportionately to the perception of the speaker’s performance. To provide novel insights on this phenomenon, we present the 3MT\_French dataset. It contains a set of public speaking annotations collected on a crowd-sourcing platform through a novel annotation scheme and protocol. Global evaluation, persuasiveness, perceived self-confidence of the speaker and audience engagement were annotated on different time windows (i.e., the beginning, middle or end of the presentation, or the full video). This new resource will be useful to researchers working on public speaking assessment and training. It will allow to fine-tune the analysis of presentations under a novel perspective relying on socio-cognitive theories rarely studied before in this context, such as first impressions and primacy and recency theories. An exploratory correlation analysis on the annotations provided in the dataset suggests that the early moments of a presentation have a stronger impact on the judgements.},
  issn      = {1574-0218}
}


